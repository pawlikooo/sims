// TODO Migrate to pug
-var ob = JSON.parse(users)
doctype html
html
  head
    title= title
    link(rel='stylesheet', href='/style.css')
    script(src="//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js")
    script(src="/ajaxHandler.js")
  body
    #container
      #header
        h1 Admin panel
      #main
        .alerts
        h2 User Info
        .userInfo
        h2 User list
        .users
          table
            thead
              tr
                th 
                  img(src= "/img/info.png")
                th UserName
                th DateCreated
                th LastLogon
                th Del
            tbody
              each user in ob
                tr(id=user.id)
                  td
                    img.infopic(src= "/img/info.png")
                  td #{user.login} 
                  td #{user.dateCreated}
                  td #{user.dateLastLogon}
                  td 
                    img.delpic(src= "/img/erase.png")
      #footer